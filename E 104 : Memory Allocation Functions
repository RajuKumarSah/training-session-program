#include<stdio.h>
#include<string.h>
#include<stdlib.h>
#include<ctype.h>
int main()
{
    char ch[1000];
    scanf("%s",ch);
    int t;
    t=strlen(ch);
    for(int i=0;i<t;i++)
    {
        if(ch[i]>='a' && ch[i]<='z')
        {
            ch[i]=ch[i]-32;
        }
        else
        {
          ch[i]=ch[i]+32;  
        }
    }
    printf("%s",ch);
  return 0;
}
