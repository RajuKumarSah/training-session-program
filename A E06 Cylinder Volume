#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int a,b;
    scanf("%d",&a);
    scanf("%d",&b);
    float t;
    t=3.14*a*a*b;
    printf("%0.2f",t);
    return 0;
}
