#include <stdio.h>

int main()
{
	int var=5;
	//scanf("%d",&var);
    int i,var1[5];
    for(i=0;i<var;i++)
    {
        scanf("%d",&var1[i]);
    }
    for(i=0;i<var;i++)
    {
        if(var1[i]%2==0)
        {
            printf("even\n");
        }
        else
        {
            printf("odd\n");
        }
    }
	return 0;
}
