#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char ch[100000],ch1[100000];
    scanf("%[^\n]s",ch);
    int i,j=0;
    for(i=0;i<strlen(ch);i++)
    {
        if(ch[i]=='a' || ch[i]=='e' || ch[i]=='i' || ch[i]=='o' || ch[i]=='u' || ch[i]=='A' || ch[i]=='E' || ch[i]=='I' || ch[i]=='O' || ch[i]=='U')
            continue;
        else
        {
            ch1[j]=ch[i];
            j++;
        }
    }
    printf("%s",ch1);
    return 0;
}
