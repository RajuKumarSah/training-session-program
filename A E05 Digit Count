#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int num; 
    scanf("%i", &num);
    int l=0;
    if(num>0)
    {
    while(num)
    {
            l++;
        num=num/10;
    }
    printf("%d",l);
    }
    else
    {
        printf("1");
    }
    return 0;
}
