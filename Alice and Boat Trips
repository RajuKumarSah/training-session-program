#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,a,b,l=0;
    scanf("%d%d%d",&n,&a,&b);
    int c[n];
    int t;
    t=a*b;
    int i;
    for(i=0;i<n;i++)
    {
        scanf("%d",&c[i]);
    }
    for(i=0;i<n;i++)
    {
        if(c[i]<=t)
        {
            l++;
        }
    }
    if(l==n)
    {
        printf("Yes\n");
    }
    else
    {
        printf("No\n");
    }
    return 0;
}
